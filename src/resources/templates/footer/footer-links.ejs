<% [ {icon: 'fa-twitter', href: "http://twitter.com"}, {icon: 'fa-facebook',
href: "http://facebook.com"}, {icon: 'fa-dribbble', href:
"http://dribbble.com"}, {icon: 'fa-instagram', href: "http://instagram.com"}
].map( (item) => { %>
<div class="col-sm-3">
    <div class="text-center">
        <a class="social" href="<%- item.href %>" rel="nofollow">
            <span class="fa <%- item.icon %> icon-md"></span>
        </a>
    </div>
</div>
<% }) %>
